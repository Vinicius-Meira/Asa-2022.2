;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	asa.br. root.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.asa.br.
@	IN	NS	ns1.asa.br.
;@	IN	NS	ns2.asa.br.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

ns1			A	10.25.1.133
mail		A 	10.25.1.133
www			CNAME	ns1
